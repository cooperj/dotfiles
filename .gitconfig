[user]
	email = hi@joshc.uk
	name = Josh Cooper
	
[oh-my-zsh]
	hide-status = 1
	hide-dirty = 1

[pull]
	rebase = false

[push]
	default = current
	autoSetupRemote = true

[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f

[init]
	defaultBranch = main

[alias]
	undo = reset --soft HEAD^